package baitapso3;

public class KiemTraSoNguyenTo {

	public static void main(String[] args) {
		{
	        int i = 1000;
	        int num = 0;
	        //Empty String
	        String  primeNumbers = "";
	        for (i = 1000; i <= 2000; i++)
	        {
	            int counter=0;
	            for(num =i; num>=1000; num--)
	            {
	                if(i%num==0)
	                {
	                    counter = counter + 1;
	                }
	            }
	            if (counter ==2)
	            {
	                //Appended the Prime number to the String
	                primeNumbers = primeNumbers + i + " ";
	            }
	        }
	        System.out.println("Cac so nguyen to tu 1000 Ä‘en 2000 la:");
	        System.out.println(primeNumbers);
	    }

	}

}
