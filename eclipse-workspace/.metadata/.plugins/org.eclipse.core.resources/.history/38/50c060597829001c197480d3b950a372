package baiso4;

public class baiso4 {
public static void main(String[] args) {
	System.out.println("Ho Va Ten: Ha Thanh Dat");
    System.out.println("MSSV: 20181057");
    System.out.println("Ma Lop: 709155");
    for(int i = 0 ; i < 100 ; i ++){
        if(i%3==0 && i%7==0){
            System.out.println("Cac so < 100 chia het cho 3 va 7 la:"+i);
        }
    }
}
}

